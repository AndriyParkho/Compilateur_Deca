class ListChaine{
	int valeur;
	ListChaine suivant;

	void setCellule(int val, ListChaine suiv){

		valeur = val;
		suivant = suiv; 
	}

	ListChaine insertPrevious(int valeur){
	ListChaine nouvelElement = new ListChaine();
	nouvelElement.setCellule(valeur, this);
	return nouvelElement;
	}

	
	void setSuivant(ListChaine suiv){
	suivant = suiv;
	}


	void printListe(){
	ListChaine current;
	current = this;
	while(current.suivant != null){
		print(current.val, ", ");
		current.setSuivant(current.suivant);
		print(current.valeur, ", ");
	}
	println(current.valeur);
	}
}

